<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/abfdnt_navigation"
    app:startDestination="@id/aboutFragment">

    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.example.fdntapp.ui.abfdnt.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" >
        <action
            android:id="@+id/action_aboutFragment_to_patronFragment"
            app:destination="@id/patronFragment" />
    </fragment>
    <fragment
        android:id="@+id/patronFragment"
        android:name="com.example.fdntapp.ui.abfdnt.PatronFragment"
        android:label="fragment_patron"
        tools:layout="@layout/fragment_patron" />
</navigation>